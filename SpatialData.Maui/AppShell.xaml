<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="SpatialData.Maui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:SpatialData.Maui.Pages"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Neighborhoods" Icon="placeholder"
        ContentTemplate="{DataTemplate pages:NeighborhoodPage}"
        Route="NeighborhoodPage" />

    <ShellContent
        Title="Nearest Subways" Icon="train"
        ContentTemplate="{DataTemplate pages:NearestSubwaysPage}"
        Route="NearestSubwaysPage" />

</Shell>
